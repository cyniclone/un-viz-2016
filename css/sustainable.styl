.sustainable /* 11 SUSTAINABLE CITIES AND COMMUNITIES */
  #map
    font: 12px sans-serif

  path
    stroke-width: 1px;
    stroke: white;
    fill: #b5b5b5;
    cursor: pointer;

  path:hover, path.highlighted
    fill: tomato;

  div.world-tooltip
    position: absolute;
    background-color: white;
    border: 1px solid black;
    color: black;
    font-weight: bold;
    padding: 4px 8px;
    display: none;